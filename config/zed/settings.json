{
  "collaboration_panel": {
    "dock": "right"
  },
  "outline_panel": {
    "dock": "right"
  },
  "chat_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "tab_bar": {
    "show": true
  },
  "show_inline_completions": false,
  "assistant": {
    "dock": "right",
    "default_model": {
      "provider": "ollama",
      "model": "llama3.1:latest"
    },
    "version": "2"
  },
  "ui_font_size": 15,
  "buffer_font_size": 14,
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Mirage"
  },
  "tab_size": 2,
  "languages": {
    "JavaScript": {
      "format_on_save": "off"
    },
    "TypeScript": {
      "format_on_save": "on"
    },
    "TSX": {
      "format_on_save": "on"
    },
    "JSX": {
      "format_on_save": "on"
    }
  },
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "experimental.theme_overrides": {
    "background.appearance": "opaque",
    "background": "#1b1b1b",
    // "background": "#D9090901",
    // "background": "#D9FFFF01",
    "editor.background": "#1b1b1b",
    "editor.gutter.background": "#1b1b1b",
    "tab_bar.background": "#00000030",
    "terminal.background": "#1b1b1b",
    "toolbar.background": "#00000030",
    "tab.active_background": "#000001",
    "tab.inactive_background": "#00000000",
    "status_bar.background": "#00000030",
    "title_bar.background": "#00000030",
    "panel.background": "#01000060",
    "border.variant": "#00000000",
    "scrollbar.track.border": "#00000000",
    "editor.active_line.background": "#00000000"
  }
}
